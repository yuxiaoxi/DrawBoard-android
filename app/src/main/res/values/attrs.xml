<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RoundProgressBar">
        <attr name="roundColor" format="color"/>
        <attr name="roundProgressColor" format="color"/>
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="max" format="integer"></attr>
        <attr name="textIsDisplayable" format="boolean"></attr>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>

    <declare-styleable name="BasicSingleItem">
        <attr format="reference" name="item_icon"/>
        <attr format="string|reference" name="item_title"/>
        <attr format="string|reference" name="item_titleHint"/>
        <attr format="dimension|reference" name="item_titleSize"/>
        <attr format="reference" name="item_titleColor"/>
        <attr format="string|reference" name="item_subTitle"/>
        <attr format="string|reference" name="item_subTitleHint"/>
        <attr format="dimension|reference" name="item_subTitleSize"/>
        <attr format="reference" name="item_subTitleColor"/>
        <attr format="string|reference" name="item_count"/>
        <attr format="dimension|reference" name="item_countSize"/>
        <attr format="reference" name="item_countColor"/>
        <attr format="reference" name="item_indicator"/>
    </declare-styleable>
</resources>